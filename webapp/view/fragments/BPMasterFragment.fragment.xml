<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No BP Found"
		title="Select Account"
		search="onHandleSearchBP"
		confirm="onHandleValueHelpCloseBatch"
		cancel="onHandleValueHelpCloseBatch"
		showClearButton="true"
		items="{oMdlAllBP>/allbp}">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{oMdlAllBP>CardCode}"
			description="Name : {oMdlAllBP>CardCode} - {oMdlAllBP>CardName}" />
	</SelectDialog>
</core:FragmentDefinition>