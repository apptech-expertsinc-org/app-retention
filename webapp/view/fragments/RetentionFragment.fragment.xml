<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Retention Found"
		title="Select Transaction"
		search="onHandleSearchRetention"
		confirm="onHandleValueHelpRetentionCloseBatch"
		cancel="onHandleValueHelpRetentionCloseBatch"
		showClearButton="true"
		items="{oMdlAllProject>/allbp}">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{oMdlAllProject>DocNum}"
			description="Transaction : {oMdlAllProject>TransType} - {oMdlAllProject>LineTotal}" />
	</SelectDialog>
</core:FragmentDefinition>