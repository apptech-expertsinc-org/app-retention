 <mvc:View controllerName="com.apptech.app-retention.controller.CreateUdtUdf"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<App id="idAppControl">
		<pages>
			<Page title="UDT and UDF Creation">
				<content>
					<Toolbar>
						<content>
							<Button id="btnUDT" text="Create UDT" tooltip="Add" press="onCreateUDT" icon="sap-icon://add" visible="{buttons>/buttons/btnUDT}" />
							<Button id="btnUDF" text="Create UDF" tooltip="Add" press="onCreateUDF" icon="sap-icon://add" visible="{buttons>/buttons/btnUDF}" />
						</content>
					</Toolbar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View> 